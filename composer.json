{
   "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:patoui/gitdown.git"
        }
    ],
    "name": "patoui/laravel-profile",
    "type": "project",
    "description": "Personal website",
    "keywords": ["patoui", "laravel"],
    "license": "MIT",
    "require": {
      "php": "^8.2",
      "ext-curl": "*",
      "ext-gd": "*",
      "ext-json": "*",
      "calebporzio/gitdown": "dev-master",
      "laravel/framework": "^11.0",
      "laravel/tinker": "^2.0",
      "laravel/ui": "^4.0",
      "predis/predis": "^1.1",
      "spatie/laravel-data": "^4.13",
      "spatie/laravel-feed": "^4.1",
      "spatie/laravel-honeypot": "^4.0",
      "spatie/laravel-tags": "^4.0"
   },
    "require-dev": {
      "barryvdh/laravel-debugbar": "^3.2",
      "barryvdh/laravel-ide-helper": "^3.5",
      "fakerphp/faker": "^1.1",
      "filp/whoops": "^2.0",
      "larastan/larastan": "^3.0",
      "laravel/pint": "^1.21",
      "mockery/mockery": "^1.6",
      "nunomaduro/collision": "^8.1",
      "phpunit/phpunit": "10.*",
      "roave/security-advisories": "dev-latest",
      "squizlabs/php_codesniffer": "*",
      "wnx/laravel-stats": "^2.1"
   },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
