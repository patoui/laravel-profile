{
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:patoui/regression-php.git"
        }
    ],
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "calebporzio/gitdown": "^1.3",
        "erusev/parsedown": "^1.6",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "intervention/image": "^2.4",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^7.0",
        "laravel/scout": "^8.0",
        "laravel/socialite": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "patoui/regression-php": "dev-master",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.0",
        "smi2/phpclickhouse": "^1.4",
        "spatie/laravel-csp": "^2.6",
        "spatie/laravel-feed": "^2.2",
        "spatie/laravel-honeypot": "^1.3",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-tags": "^2.5",
        "twilio/sdk": "5.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "^0.7.0",
        "phpunit/phpunit": "9.*",
        "psalm/plugin-laravel": "^1.1",
        "squizlabs/php_codesniffer": "*",
        "vimeo/psalm": "^3.4",
        "wnx/laravel-stats": "^2.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": ["app/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@php hooks/setup.php"
        ],
        "post-update-cmd": [
            "@php hooks/setup.php"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
